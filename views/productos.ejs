<%- include('templates/header') %>
<%- include('templates/nav') %>


<main>
    <h2>Productos</h2>
    <section>
        <% if (productos.length > 0) { %>
            <% productos.forEach(({descripcion, producto, disponible}) => { %>

                <% if (disponible) { %>
                    <article>
                        <h2><%= producto %></h2>
                        <p><%= descripcion %></p>
                    </article>
                    <% } %>
               
                    
            <% }) %> <% } else { %>
             <p>Productos no disponibles</p>

        <%}%> 
    </section>
</main>

<%- include('templates/footer') %>